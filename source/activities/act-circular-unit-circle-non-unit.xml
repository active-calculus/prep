<worksheet label="ws-act-circular-unit-circle-non-unit">
  <activity xml:id="act-circular-unit-circle-non-unit">
    <introduction>
      <p>
			Determine each of the following values or points exactly.
		</p>
      <p/>
    </introduction>
    <task workspace="3cm" xml:id="act-circular-unit-circle-non-unit-task-0">
      <statement>
        <p>
		            In a circle of radius <m>11</m>, the arc length intercepted by a central angle of <m>\frac{5\pi}{3}</m>.
		          </p>
      </statement>
      <hint/>
      <answer/>
      <solution/>
    </task>
    <task workspace="3cm" xml:id="act-circular-unit-circle-non-unit-task-1">
      <statement>
        <p>
		            In a circle of radius <m>3</m>, the central angle measure that intercepts an arc of length <m>\frac{\pi}{4}</m>.
		          </p>
      </statement>
      <hint/>
      <answer/>
      <solution/>
    </task>
    <task workspace="3cm" xml:id="act-circular-unit-circle-non-unit-task-2">
      <statement>
        <p>
		            The radius of the circle in which an angle of
		            <m>\frac{7\pi}{6}</m> intercepts an arc of length <m>\frac{\pi}{2}</m>.
		          </p>
      </statement>
      <hint/>
      <answer/>
      <solution/>
    </task>
    <task workspace="3cm" xml:id="act-circular-unit-circle-non-unit-task-3">
      <statement>
        <p>
		        		The exact coordinates of the point on the circle of radius <m>5</m> that lies <m>\frac{25\pi}{6}</m> units counterclockwise along the circle from <m>(5,0)</m>.
		        	</p>
      </statement>
      <hint/>
      <answer/>
      <solution/>
    </task>
    <answer>
      <p/>
    </answer>
    <solution>
      <p/>
    </solution>
  </activity>
</worksheet>
